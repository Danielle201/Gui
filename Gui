local ScreenGui1 = Instance.new("ScreenGui") 
ScreenGui1.Parent = game.CoreGui

local Frame1 = Instance.new("Frame") 
Frame1.Parent = ScreenGui1
Frame1.Name = "Frame"
Frame1.BackgroundColor3 = Color3.fromRGB(242,243,243)
Frame1.BorderSizePixel = 5
Frame1.BorderColor3 = Color3.fromRGB(0,0,255)
Frame1.Size = UDim2.new(0.5,0,0.5)
Frame1.Position = UDim2.new(0.5,0,0)
Frame1.Active = true
Frame1.Draggable = true

local TextLabel1 = Instance.new("TextLabel") 
TextLabel1.Size = UDim2.new(0.4,0,0.2)
TextLabel1.Font = Enum.Font.Bangers
TextLabel1.Parent = Frame1
TextLabel1.Name = "Text"
TextLabel1.BorderSizePixel = 5
TextLabel1.BorderColor3 = Color3.fromRGB(0,0,255)
TextLabel1.BackgroundTransparency = 1
TextLabel1.Position = UDim2.new(0.3,0,0)
TextLabel1.Text = "Change WalkSpeed"
TextLabel1.TextSize = 30
TextLabel1.Active = true
TextLabel1.Draggable = false
TextLabel1.TextColor3 = Color3.fromRGB(0,0,255)
TextLabel1.TextScaled = true
TextLabel1.TextWrapped = true

local TextLabel2 = Instance.new("TextLabel") 
TextLabel2.Size = UDim2.new(0.4,0,0.2)
TextLabel2.Font = Enum.Font.Bangers
TextLabel2.Parent = Frame1
TextLabel2.Name = "Text"
TextLabel2.BorderSizePixel = 5
TextLabel2.BorderColor3 = Color3.fromRGB(0,0,255)
TextLabel2.BackgroundTransparency = 1
TextLabel2.Position = UDim2.new(0.3,0,0.15)
TextLabel2.Text = "Click Button Many Times To Work"
TextLabel2.TextSize = 30
TextLabel2.Active = true
TextLabel2.Draggable = false
TextLabel2.TextColor3 = Color3.fromRGB(0,0,255)
TextLabel2.TextScaled = true
TextLabel2.TextWrapped = true

local TextButton1 = Instance.new("TextButton") 
TextButton1.Parent = Frame1
TextButton1.Name = "Walkspeed button"
TextButton1.BackgroundColor3 = Color3.fromRGB(0,0,255)
TextButton1.BorderSizePixel = 5
TextButton1.BorderColor3 = Color3.fromRGB(0,0,0)
TextButton1.Position = UDim2.new(0.1,0,0.4)
TextButton1.Size = UDim2.new(0.8,0,0.2)
TextButton1.TextSize = 30
TextButton1.TextScaled = true
TextButton1.TextWrapped = true
TextButton1.Text = "100"
TextButton1.Font = Enum.Font.Arcade

local TextButton2 = Instance.new("TextButton") 
TextButton2.Parent = Frame1
TextButton2.Name = "Reset button"
TextButton2.BackgroundColor3 = Color3.fromRGB(0,0,255)
TextButton2.BorderSizePixel = 5
TextButton2.BorderColor3 = Color3.fromRGB(0,0,0)
TextButton2.Position = UDim2.new(0.1,0,0.6)
TextButton2.Size = UDim2.new(0.8,0,0.3)
TextButton2.TextSize = 30
TextButton2.TextScaled = true
TextButton2.TextWrapped = true
TextButton2.Text = "Reset Speed"
TextButton2.Font = Enum.Font.Arcade

TextButton1.MouseButton1Click:Connect(function() 
while wait() do
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
end
end)

TextButton2.MouseButton1Click:Connect(function() 
while wait() do
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
